{
  "expo": {
    "name": "barcode-scanner",
    "slug": "barcode-scanner",
    "version": "1.0.0",
    "scheme": "barcode-scanner",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chauhanyuvraj.barcodescanner"
    },
    "android": {
      "package": "com.chauhanyuvraj.barcodescanner",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "88508cf7-2959-459f-9745-6781ad04ce28"
      }
    }
  }
}
